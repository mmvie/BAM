  <header class="banner-header">
    <div class="page-visuel-wrapper">
      <img class="banner-visual desktop-visual" typeof="foaf:Image" src="https://www.scor.com/sites/default/files/styles/desktop_banner/public/lh-bam-tetiere_desktop.png?itok=yT3d00ZS" width="520" height="330" alt="" />      <img class="banner-visual mobile-visual" typeof="foaf:Image" src="https://www.scor.com/sites/default/files/styles/mobile_banner/public/lh-bam-tetiere_mobile.png?itok=DpNC2C_b" width="160" height="145" alt="" />    </div>
